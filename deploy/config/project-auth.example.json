{
  "default_endpoint": "https://cloud.appwrite.io/v1",
  "projects": {
    "PROJECT_A": {
      "api_key": "sk_example_project_a",
      "aliases": ["prod", "main"],
      "default_for_auto": true
    },
    "PROJECT_B": {
      "api_key": "sk_example_project_b"
    }
  },
  "defaults": {
    "auto_target_project_ids": ["PROJECT_A"],
    "target_selector": {
      "mode": "auto"
    }
  },
  "management": {
    "endpoint": "https://cloud.appwrite.io/v1",
    "api_key": "sk_example_management",
    "project_id": "console"
  }
}
